 @{
    ViewData["Title"] = "View Chefs";
 }
 @model List<Chef>

 <div class="text-center">
   <nav>
       <h4><Span><a href="/">Home</a></Span> | <span><a href="/viewchefs">Chefs</a></span> | <span> <a href="/viewdishes">Dishes</a></span> |  <span><a href="/addchef">Add a Chef</a></span> | <span><a href="/adddish">Add A Dish</a></span></h4>
    </nav>
    <h4> Check out our  roster of Chefs!!!!</h4>

<table>

        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">age</th>
                <th scope="col"># of Dishes</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach(Chef c in Model)
            {
                <tr>
                    
                   <td>@c.FullName()</td> @* we can also type this in our cshtml file @c.FirstName @c.LastName  *@
                    <td>@c.Age()</td>
                    <td>@c.AllDishes.Count</td>
                </tr>
            }
        <tbody>
</table>
